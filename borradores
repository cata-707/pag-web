}

/* Cambiar el color del texto de la barra de navegación */
.navbar .nav-link {
  color: #ffffff;
}

.navbar .nav-link:hover {
  color: #f0ad4e;
  /* Cambia el color cuando el usuario pase el cursor sobre el enlace */
}

/* Cambiar el color del texto del botón activo */
.navbar .nav-link.active {
  color: #ffffff;
  /* Cambia esto al color que desees para el enlace activo */
}

/* Cambiar el color del texto del botón deshabilitado */
.navbar .nav-link.disabled {
  color: #cccccc;
  /* Cambia esto al color que desees para el enlace deshabilitado */
}

/* Centrar el formulario en la barra de navegación */
.navbar-collapse {
  display: flex;
  justify-content: center;
  /* Centra el contenido horizontalmente */
}

form.d-flex {
  width: 50%;
  /* Ajusta el ancho del formulario si es necesario */
}

/* Estilo para los iconos dentro de los botones */
.navbar .btn i {
  color: white;
  /* Color del icono por defecto */
  font-size: 1.25rem;
  /* Tamaño del icono (ajusta según tu necesidad) */
}

/* Estilo para los botones con iconos */
.navbar .btn {
  border: none;
  /* Eliminar el marco del botón */
  background-color: transparent;
  /* Fondo transparente */
}

/* Cambiar color del icono al pasar el mouse */
.navbar .btn:hover i {
  color: #0d6efd;
  /* Cambia a un color deseado (por ejemplo, azul de Bootstrap) */
}

/* Cambiar color del botón al pasar el mouse */
.navbar .btn:hover {
  background-color: rgba(0, 123, 255, 0.1);
  /* Fondo semitransparente cuando se pasa el mouse (ajusta el color si es necesario) */
}





























